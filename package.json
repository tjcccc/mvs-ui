{
  "name": "mvsui",
  "project": "mvsui",
  "description": "An administration UI framework.",
  "version": "0.4.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "dir": "npm ls --depth=0",
    "scss": "scss --update --style compressed app:app scss/default/mvsui.scss:style/default/default.css",
    "scss watch": "scss --watch --style compressed app:app scss/default/mvsui.scss:style/default/default.css"
  },
  "keywords": [
    "mvsui",
    "angular2",
    "bootstrap",
    "administration",
    "framework"
  ],
  "homepage": "https://github.com/tjcccc/mvsui",
  "author": "seesaw game",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjcccc/mvsui.git"
  },
  "bugs": {
    "url": "https://github.com/tjcccc/mvsui/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",
    
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.3"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "typings":"^1.3.2"
  },
  "private": true
}
