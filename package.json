{
    "name": "mvsui",
    "project": "mvsui",
    "description": "An administration UI framework.",
    "version": "0.5.0",
    "scripts": {
        "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
        "lite": "lite-server",
        "postinstall": "typings install",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "typings": "typings",
        "dir": "npm ls --depth=0",
        "scss": "scss --update --style compressed app:app scss/default/mvsui.scss:style/default/default.css",
        "scss watch": "scss --watch --style compressed app:app scss/default/mvsui.scss:style/default/default.css"
    },
    "keywords": [
        "mvsui",
        "angular2",
        "bootstrap",
        "administration",
        "framework"
    ],
    "homepage": "https://github.com/tjcccc/mvsui",
    "author": "seesaw game",
    "repository": {
        "type": "git",
        "url": "https://github.com/tjcccc/mvsui.git"
    },
    "bugs": {
        "url": "https://github.com/tjcccc/mvsui/issues"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
        }
    ],
    "dependencies": {
        "@angular/common": "~2.2.0",
        "@angular/compiler": "~2.2.0",
        "@angular/core": "~2.2.0",
        "@angular/forms": "~2.2.0",
        "@angular/http": "~2.2.0",
        "@angular/platform-browser": "~2.2.0",
        "@angular/platform-browser-dynamic": "~2.2.0",
        "@angular/router": "~3.2.0",
        "@angular/upgrade": "~2.2.0",
        "angular-in-memory-web-api": "~0.1.15",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.8",
        "rxjs": "5.0.0-beta.12",
        "systemjs": "0.19.39",
        "zone.js": "^0.6.25",
        "bootstrap": "^3.3.6",
        "font-awesome": "^4.6.3"
    },
    "devDependencies": {
        "@types/core-js": "^0.9.34",
        "@types/node": "^6.0.45",
        "concurrently": "^3.0.0",
        "lite-server": "^2.2.2",
        "typescript": "^2.0.3"
    },
    "private": true
}
